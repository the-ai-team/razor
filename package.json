{
  "name": "razor",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "start:server": "nx serve server",
    "build": "nx build client && nx build server",
    "test": "nx run-many --all --target=test && npm run coverage:merge && npm run coverage:summary",
    "coverage:codecov": "codecov",
    "coverage:merge": "npx istanbul-merge --out ./coverage/coverage-final.json ./coverage/libs/*/coverage-final.json",
    "coverage:summary": "npx istanbul report --include ./coverage/coverage-final.json --dir ./coverage json-summary clover",
    "test:store": "nx test store --watchAll",
    "test:util": "nx test util --watchAll",
    "build:client": "nx run client:build:production",
    "build:server": "nx run server:build:production",
    "serve": "node dist/apps/server/main.js",
    "pretty": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md,html}\"",
    "lint": "nx run-many --all --target=lint",
    "make-badges": "istanbul-badges-readme --coverageDir='./coverage'  --logo='jest'",
    "make-badges:ci": "npm run make-badges -- --ci --exitCode=1"
  },
  "private": true,
  "engines": {
    "node": ">=16.17.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "@rematch/core": "^2.2.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "pixelarticons": "^1.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.4",
    "regenerator-runtime": "0.13.7",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "tslib": "^2.3.0",
    "util": "^0.12.5",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nrwl/cli": "15.0.0",
    "@nrwl/cypress": "15.0.0",
    "@nrwl/eslint-plugin-nx": "15.0.0",
    "@nrwl/jest": "15.0.0",
    "@nrwl/linter": "15.0.0",
    "@nrwl/node": "15.0.0",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/react": "15.0.0",
    "@nrwl/web": "15.0.0",
    "@nrwl/workspace": "15.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
    "@types/lodash": "^4.14.189",
    "@types/node": "16.11.7",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "10.4.12",
    "babel-jest": "28.1.1",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.5",
    "cypress": "^10.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.25.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "husky": "^8.0.1",
    "istanbul": "^0.4.5",
    "istanbul-badges-readme": "^1.8.4",
    "istanbul-merge": "^1.1.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "^13.0.3",
    "nx": "15.0.0",
    "nyc": "^15.1.0",
    "postcss": "8.4.16",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.1.8",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{scss,md,html,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
